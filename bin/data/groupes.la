!	groupes.la
!- ter|grp:<groupe>.<id>		// identifiant syntaxique,identifiant unique 
!- pos:<pos>					// liste, sép " "
!- morph:<traits morphos>		// liste sép " " et ";"
!- lexSynt:<lexicosyntaxe>		// liste, sép " "
!// a, ag :mots et groupes avant le noyau
!- a:pos;id-lien;traita traitb,traita traitb;accord;lexsynt lexsynt...
!- ag: "  "        "     "		// comme a, pour les groupes
!// p, pg :mots et groupes après le noyau
!- p:   "     "    "      "		// sub après le noyau
!- pg:   "     "    "      "	// comme p, pour les groupes
!
!phrases de test :
!  Prometheus Iapeti filius homines ex luto finxit.
!  Aesculapius Apollinis filius Glauco Minois filio vitam reddidit.
!
!CAVEAT : il faut terminer par un groupe non terminal
!

! Groupes terminaux

ter:n.prepAbl
pos:n NP
morph:abl
a:prepAbl prepAA;prep

ter:n.prepAcc
pos:n NP
morph:acc
a:prepAA prepAcc;prep

ter:n.prepAdjAcc
pos:a
morph:acc
!lexSynt:subst
a:prepAA prepAcc;prep

ter:n.fam
pos:n
lexSynt:fam
a:NP;gen;gén

ter:n.genacc
pos:n
morph:acc
a:n;gen;gén

ter:n.genabl
pos:n
morph:abl
a:NP n;gen;gén

! Groupes non terminaux

grp:n.app
pos:NP
pg:n.fam;app;;cgn

grp:v.objprep
pos:v
morph:act
!lexSynt:trans
a:n;obj;acc
ag:n.prepAbl;gprep

!grp:v.datobj
!pos:v
!morph:act
!lexSynt:datif
!ag:n;dat;dat
!ag:n;obj;acc

grp:P.1
pos:v.objprep
morph:indic 3;subj 3
ag:n;sujet;nomin;n

grp:P.2
pos:v
morph:act
a:Adv;adv
a:n NP;sujet;nomin;n
ag:n.genabl;abl
ag:n.prepAbl;gprep
ag:n.genacc;obj

grp:P.3
pos:v
morph:act
a:n NP;sujet;nomin;n
a:Adv;adv
ag:n.prepAbl;gprep
a:n;obj;acc

grp:P.4
pos:v
morph:act
a:neg;neg
ag:n.prepAdjAcc;gprep
!- a:pos;id-lien;traita traitb,traita traitb;accord;lexsynt lexsynt...
a:v;obj;infin
