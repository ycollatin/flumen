grp:v.obj
pos:v
morph:act
lexsynt:!intr
ag:n NP p !n.prepAcc !n.prepAbl !n.inperpetuum;obj;acc

grp:v.dienocte
pos:v
ag:"dies" "nox" "hora";abl;abl

grp:n.gen
pos:n
